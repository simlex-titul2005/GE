@model VMSeoInfo[]
@{
    Layout = null;
    var pagerInfo = ViewBag.PagerInfo;
    var filter = ViewBag.Filter != null ? (VMSeoInfo)ViewBag.Filter : null;
    var sortDirections = ViewBag.Order != null ? (Dictionary<string, SxExtantions.SortDirection>)ViewBag.Order : null;
}


@Html.SxGridView(
        new SxExtantions.SxGridViewSettings<VMSeoInfo>
    {
        UpdateTargetId = "grid-seo",
        Data = Model,
        Columns = new SxExtantions.SxGridViewColumn[]{
            new SxExtantions.SxGridViewColumn{FieldName="RawUrl", Caption="Адрес"}
        },
        PagerInfo=pagerInfo,
        Filter = filter,
        SortDirections = sortDirections
    },
        new { @class = "table table-condensed table-bordered table-responsive table-striped" }
    )