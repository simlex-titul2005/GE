@model SxVMFAQ[]
@{
    ViewBag.Title = "FAQ";
    SxVMFAQ item;
}

<h2>@ViewBag.Title</h2>

<div class="row">
    <div class="col-md-2">
        @Html.Action("TreeViewMenu", "MaterialCategories", new { mct = Enums.ModelCoreType.Manual })
    </div>
    <div class="col-md-10">
        @if (Model.Any())
        {
            for (int i = 0; i < Model.Length; i++)
            {
                item = Model[i];
                @Html.DisplayFor(x => item)
            }
        }
    </div>
</div>