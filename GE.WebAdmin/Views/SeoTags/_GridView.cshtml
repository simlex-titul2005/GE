@model SxVMSeoTags[]
@{
    Layout = null;
}


@Html.SxGridView(Model,
        new SxExtantions.SxGridViewSettings<SxVMSeoTags>
        {
            Columns = new SxExtantions.SxGridViewColumn<SxVMSeoTags>[]{
                new SxExtantions.SxGridViewColumn<SxVMSeoTags>{FieldName="RawUrl", Caption="Адрес"}
