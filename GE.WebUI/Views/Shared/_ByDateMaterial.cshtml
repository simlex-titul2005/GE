@model GE.WebUI.Models.Abstract.VMLastMaterial[]
@{
    Layout = null;
}

<div class="by-date-m">
    <div class="row">
        @for (int i = 0; i < Model.Length; i++)
        {
            var item = Model[i];
            <div class="col-md-4">
                <div class="content">
                    <div><time>@item.DateOfPublication</time></div>
                    <h4>@item.Title</h4>
                    <div>@item.Foreword</div>
                </div>
            </div>
        }
    </div>
</div>