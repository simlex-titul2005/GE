@model VMFAQ[]
@{
    ViewBag.Title = "FAQ";
}

<h2>@ViewBag.Title</h2>

<div class="row">
    <div class="col-md-2">
        @Html.Action(MVC.MaterialCategories.TreeViewMenu(mct: SX.WebCore.Enums.ModelCoreType.Manual))
    </div>
    <div class="col-md-10">
        @if(Model.Any())
        {
            for (int i = 0; i < Model.Length; i++)
            {
                var faq = Model[i];
                @Html.DisplayFor(x=>faq)
            }
        }
    </div>
</div>