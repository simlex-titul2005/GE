@model VMImgGameMaterial[]
@{
    Layout = null;
}

<div class="glm">
    @if (Model.Any())
    {
        <ul>
            @for (int i = 0; i < Model.Length; i++)
            {
                var item = Model[i];
                
                <li>
                    <a href="#">@item.Title</a>
                </li>
            }
        </ul>
    }
</div>