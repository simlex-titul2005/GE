@model VMPicture[]
@{
    Layout = null;
}
<table class="table table-condensed table-responsive table-hover table-bordered">
    @for (int i = 0; i < Model.Length; i++)
    {
        var item = Model[i];
        <tr data-picture-id="@item.Id">
            <td>@item.Width x @item.Height</td>
            <td>@item.ImgFormat</td>
            <td>@item.Caption</td>
        </tr>
    }
</table>

