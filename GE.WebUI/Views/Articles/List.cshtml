@model SxExtantions.SxPagedCollection<Article>
@{
    ViewBag.Title = ViewBag.Title ?? "Статьи";
    if (ViewBag.GameTitle != null)
    {
        ViewBag.Title += " " + ViewBag.GameTitle;
    }
    ViewBag.H1 = ViewBag.H1 ?? ViewBag.Title;
    ViewBag.H1CssClass = "header";
}

@Html.Partial(MVC.Shared.Views._Breadcrumbs, (VMBreadcrumb[])ViewBag.Breadcrumbs)

@Html.Partial(MVC.Articles.Views._Collection, Model)